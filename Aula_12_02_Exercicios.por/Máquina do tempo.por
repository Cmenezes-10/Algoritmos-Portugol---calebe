programa 
{
  funcao inicio()
  {
    inteiro n1
     escreva ("Escolha uma opção (1, 2 ou 3): ")
     leia (n1)

     escolha (n1)
     {
      caso 1:
      {
        escreva ("Época dos dinossauros")
        pare
      }

      caso 2:
      {
        escreva ("Idade Média")
        pare
      }

      caso 3:
      {
        escreva ("Futuro Cyberpunk")
        pare
      }

      caso contrario:
      {
        escreva ("Vácuo temporal")
      }
     }
  }
}
